{
	"name": "front-end-test-2",
	"version": "1.0.0",
	"description": "Teste desenvolvido por @cicerovinicius para vaga de front-end na Red Ventures",
	"main": "index.html",
	"scripts": {
		"dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
		"watch": "npm run dev -- --watch",
		"prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
		"start": "parcel index.html --open"
	},
	"dependencies": {
		"jquery": "^3.4.1",
		"parcel": "1.12.4",
		"popper.js": "^1.16.1",
		"slick-carousel": "^1.8.1",
		"smoothscroll-for-websites": "^1.4.9"
	},
	"devDependencies": {
		"copy-webpack-plugin": "^5.0.3",
		"cross-env": "^7.0",
		"image-webpack-loader": "^5.0.0",
		"imagemin-mozjpeg": "^6.0.0",
		"imagemin-webpack-plugin": "^2.4.2",
		"laravel-mix": "^5.0.1",
		"laravel-mix-clean-css": "^1.3.1",
		"laravel-mix-imagemin": "^1.0.3",
		"lodash": "^4.17.19",
		"parcel-bundler": "^1.6.1",
		"resolve-url-loader": "^3.1.0",
		"sass": "^1.15.2",
		"sass-loader": "^8.0.0"
	},
	"keywords": []
}
